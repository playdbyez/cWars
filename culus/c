#include <stdio.h>
#include <malloc.h>
#include <string.h>

char *strsum(const char *a, const char *b)
{

  int len1 = strlen(a);
  int len2 = strlen(b);
  int max  = len1+len2;
  int iter = len1 < len2 ? len1 : len2;
  
  char *lenni;
  lenni = (char *) malloc (sizeof (char)*max);
  
  
  int  path = 1;
  int carry = 0;
  int sum = 0;
  char buffsum[3];
  
  for (unsigned long long i  = len1-path; i > 0; i--){
    for (unsigned long long j  = len2-path; j > 0; j--){
        sum = a[len1] + b[len2]; 
      
    }
  }
  
  
  sprintf(lenni,"%ld", len1);
  
    return lenni;
}
